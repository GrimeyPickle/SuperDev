{
	"name": "superdev",
	"version": "1.1.2",
	"private": true,
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-scripts": "5.0.1"
	},
	"scripts": {
		"build": "react-scripts build",
		"move": "mv -v build/static/* build/ && mv build/js/main.*.js build/js/main.js && mv build/css/main.*.css build/css/main.css",
		"copy": "cp -R src/icons src/libs manifest.json rules.json build && cp -R src/css/cs.css build/css && cp -R src/js/* build/js",
		"remove": "rm -r build/asset-manifest.json build/js/*.txt build/libs/css/font-awesome build/static",
		"renameCSS": "sed -i '' 's~/static/media/~chrome-extension://__MSG_@@extension_id__/media/~g' build/css/main.css",
		"renameHTML": "sed -i '' 's~/static/js/main.*.js~/js/main.js~g' build/index.html && sed -i '' 's~/static/css/main.*.css~/css/main.css~g' build/index.html"
	},
	"eslintConfig": {
		"extends": [
			"react-app"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"autoprefixer": "^10.4.13",
		"css-declaration-sorter": "^6.3.1",
		"eslint": "^8.23.1",
		"eslint-plugin-react": "^7.31.8",
		"postcss": "^8.4.18",
		"postcss-colornames-to-hex": "^1.0.1",
		"postcss-discard-comments": "^5.1.2",
		"postcss-discard-duplicates": "^5.1.0",
		"postcss-discard-empty": "^5.1.1",
		"postcss-discard-overridden": "^5.1.0",
		"postcss-discard-unused": "^5.1.0",
		"postcss-import-url": "^7.0.0",
		"postcss-merge-longhand": "^5.1.7",
		"postcss-merge-rules": "^5.1.3",
		"postcss-ordered-values": "^5.1.3",
		"postcss-selector-parser": "^6.0.10",
		"postcss-unique-selectors": "^5.1.1",
		"renamer": "^4.0.0",
		"tailwindcss": "^3.1.8"
	}
}
