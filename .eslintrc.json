{
  "env": {
    "browser": true,
    "webextensions": true,
    "es2021": true,
    "jquery": true
    
  },
  "globals": {
    "window": true,
    "module": true,
    "js_beautify" : true,
    "css_beautify" : true,
    "html_beautify" : true

  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-unused-vars": "off",
    "react/prop-types": "off",
    "no-mixed-spaces-and-tabs": "off"
  }
}