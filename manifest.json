{
	"name": "SuperDev",
	"version": "1.0.0",
	"description": "A Browser Extension and a Swiss Army Toolbox for Web Designers, Developers and Founders.",
	"permissions": [
		"storage",
		"tabs",
		"activeTab",
		"scripting",
		"webRequest",
		"webNavigation",
		"privacy",
		"contextMenus",
		"cookies",
		"browsingData",
		"history",
		"notifications",
		"desktopCapture",
		"clipboardWrite",
		"unlimitedStorage",
		"nativeMessaging",
		"alarms",
		"fontSettings",
		"contentSettings",
		"proxy"
	],
	"host_permissions": ["*://*/*"],
	"background": {
		"service_worker": "js/background.js"
	},
	"author": "Sixsilicon",
	"action": {
		"default_title": "SuperDev"
	},
	"icons": {
		"16": "icons/orange/icon16.png",
		"48": "icons/orange/icon48.png",
		"128": "icons/orange/icon128.png"
	},
	"content_scripts": [
		{
			"js": ["libs/js/jquery.min.js", "libs/js/jquery-ui.min.js", "js/content.js"],
			"matches": ["file://*/*", "http://*/*", "https://*/*"]
		}
	],
	"web_accessible_resources": [
		{
			"matches": ["file://*/*", "http://*/*", "https://*/*"],
			"resources": ["index.html", "404.html", "css/*", "fonts/*", "icons/*", "js/*", "libs/*", "next/*"]
		}
	],
	"manifest_version": 3,
	"minimum_chrome_version": "88"
}
