{
	"name": "SuperDev Pro",
	"version": "1.0.0",
	"description": "A Swiss Army Toolbox Browser Extension for Web Designers, Developers and Founders.",
	"permissions": ["storage", "tabs", "activeTab", "scripting", "contextMenus", "clipboardWrite", "desktopCapture", "downloads", "management"],
	"host_permissions": ["http://*/*", "https://*/*"],
	"background": {
		"service_worker": "js/background.js"
	},
	"commands": {
		"_execute_action": {
			"suggested_key": {
				"chromeos": "Ctrl+Shift+S",
				"linux": "Ctrl+Shift+S",
				"mac": "Ctrl+Shift+S",
				"windows": "Ctrl+Shift+S"
			}
		}
	},
	"author": "Choudhary Abdullah",
	"action": {
		"default_title": "SuperDev Pro"
	},
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"content_scripts": [
		{
			"js": ["js/content.js", "libs/js/jquery.min.js", "libs/js/jquery-ui.min.js"],
			"matches": ["http://*/*", "https://*/*"]
		}
	],
	"web_accessible_resources": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"resources": ["icons/*", "js/*", "libs/*", "static/*"]
		}
	],
	"manifest_version": 3,
	"minimum_chrome_version": "88"
}
