{
	"name": "SuperDev",
	"version": "1.0.0",
	"description": "SuperDev is Chrome Extension for Developers and Designers that Adds 20+ Extension Feature into a Single one.",
	"permissions": ["storage", "tabs", "activeTab", "scripting", "webRequest", "webNavigation"],
	"background": {
		"service_worker": "js/background.js"
	},
	"author": "Sixsilicon",
	"action": {
		"default_title": "SuperDev",
		"default_icon": {
			"16": "icons/icon16.png",
			"48": "icons/icon48.png",
			"128": "icons/icon128.png"
		}
	},
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"web_accessible_resources": [
		{
			"matches": ["file:///*", "http://*/*", "https://*/*"],
			"resources": ["css/*", "fonts/*", "icons/*", "js/*", "libs/*", "popups/*"]
		}
	],
	"manifest_version": 3
}
