{
	"name": "SuperDev",
	"version": "1.0.5",
	"author": "Choudhary Abdullah",
	"description": "A Swiss Army Toolbox for Web Designers and Developers.",
	"manifest_version": 3,
	"minimum_chrome_version": "88",
	"permissions": ["storage", "activeTab", "contextMenus", "tabs", "scripting", "management", "browsingData"],
	"host_permissions": ["http://*/*", "https://*/*"],
	"background": {
		"service_worker": "js/bg.js"
	},
	"commands": {
		"_execute_action": {
			"suggested_key": {
				"chromeos": "Ctrl+Shift+S",
				"linux": "Ctrl+Shift+S",
				"mac": "Ctrl+Shift+S",
				"windows": "Ctrl+Shift+S"
			}
		},
		"clearAllCache": {
			"description": "Clear All Cache",
			"suggested_key": {
				"chromeos": "Ctrl+Shift+E",
				"linux": "Ctrl+Shift+E",
				"mac": "Ctrl+Shift+E",
				"windows": "Ctrl+Shift+E"
			}
		}
	},
	"action": {
		"default_title": "SuperDev"
	},
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"content_scripts": [
		{
			"css": ["css/tabs.css"],
			"js": [
				"libs/js/jquery.min.js",
				"libs/js/jquery-ui.min.js",
				"libs/js/beautify.min.js",
				"libs/js/beautify-css.min.js",
				"libs/js/beautify-html.min.js",
				"js/cs.js"
			],
			"matches": ["http://*/*", "https://*/*"]
		}
	],
	"web_accessible_resources": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"resources": ["index.html", "css/*", "icons/*", "js/*", "libs/*", "next/*"]
		}
	]
}
